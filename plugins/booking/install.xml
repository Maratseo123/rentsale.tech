        <page key="booking_terms_and_conditions" name="Booking Rules and Policies" type="static" controller="static" path="booking-terms-and-conditions" version="3.0.0" tpl="1"><![CDATA[Terms specific for Hosts and Guests.]]></page>
    </pages>

    <emails>
        <email version="3.0.0" key="booking_accepted_request" subject="Your booking request accepted" type="html">
            <![CDATA[
                Hello {renter},<br>
                Your booking request has been successfully accepted.
                {if period_updated}<br>
                {owner} has rescheduled the dates; please find updated details below:
                <br>Date(s): {date}
                <br>Total: {amount}{/if}<br>
                Listing: {link}
                Comment: {BODY}
                ______________________________
                Thank you,
                {site_name}
            ]]>
        </email>
        <email version="3.0.0" key="booking_refused_request" subject="Your booking request rejected" type="html">
            <![CDATA[
                Hello {renter},<br>
                Unfortunately, {owner} has rejected your booking request.
                {if period_updated}<br>
                {owner} has rescheduled the dates; please find updated details below:
                <br>Date(s): {date}
                <br>Total: {amount}{/if}<br>
                Listing: {link}
                Reason: {BODY}
                ______________________________
                Thank you,
                {site_name}
            ]]>
        </email>
        <email version="3.0.0" key="booking_new_request_notify" subject="A new booking request" type="html">
            <![CDATA[
                You've got a new booking request on {date}<br>
                <b>Booking Details</b>
                Listing: {link}
                Check-in: {checkin}
                Check-out: {checkout}
                Total: {amount}
                {prepayment}<br>
                <b>Customer Details</b>
                {details}
                ______________________________
                Thank you,
                {site_name}
            ]]>
        </email>
        <email version="3.0.0" key="booking_accepted_request_escort" subject="Your booking request accepted" type="html">
            <![CDATA[
                Hello {renter},<br>
                Your booking request has been successfully accepted.
                {if period_updated}<br>
                {owner} has rescheduled the time; please find updated details below:
                <br>Date: {date}
                <br>Total: {amount}{/if}<br>
                Profile: {link}
                Comment: {BODY}
                ______________________________
                Thank you,
                {site_name}
            ]]>
        </email>
        <email version="3.0.0" key="booking_refused_request_escort" subject="Your booking request rejected" type="html">
            <![CDATA[
                Hello {renter},<br>
                Unfortunately, {owner} has rejected your booking request.
