    <blocks>
        <block version="3.0.0" key="booking_availability_block" name="Search for Reservations" side="left" type="smarty" tpl="1"><![CDATA[
            {include file=$smarty.const.RL_PLUGINS|cat:'booking/availability_block.tpl'}
        ]]></block>
        <block version="3.0.0" key="booking_calendar_box" name="Calendar" side="left" type="smarty" tpl="1"><![CDATA[
            {include file=$smarty.const.RL_PLUGINS|cat:'booking/booking_group.tpl' displayCalendar=true}
        ]]></block>
        <block version="3.0.0" key="booking_order_details_1" name="Booking Details (listing)" side="left" type="smarty" tpl="1" header="0"><![CDATA[
            {include file=$smarty.const.RL_PLUGINS|cat:'booking/smarty_blocks/listing_data.tpl'}
        ]]></block>
        <block version="3.0.0" key="booking_order_details_2" name="Booking Details (legend and ranges)" side="left" type="smarty" tpl="0" header="0"><![CDATA[
            {include file=$bookingBlocksFolder|cat:'rate_ranges.tpl'}
